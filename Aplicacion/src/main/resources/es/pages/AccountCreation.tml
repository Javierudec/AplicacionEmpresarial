<html t:type="layout" title="Hi/Lo Guess"
  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">
 
<!-- 
<t:beaneditform submitlabel="Create account"
				object="userInformation" 
                reorder="name,password,email"/>
-->

<div id="defaultMenu">      
	<table>
		<t:Form>
			<t:Errors/>
			<tr>
			<td><t:Label t:for="newUsername">Username</t:Label></td><td><input t:type="Textfield" t:id="newUsername" t:validate="required" size="24"/></td>
			</tr>
			<tr>
			<td><t:Label t:for="newPassword">Password</t:Label></td><td><input t:type="PasswordField" t:id="newPassword" t:validate="required" size="24"/></td>
			</tr>
			<tr>
			<td><t:Label t:for="newEMail">Email</t:Label></td><td><input t:type="Textfield" t:id="newEMail" t:validate="required" size="24"/></td>
			</tr>
			<tr>
			<td></td><td><input type="submit" value="Create"/></td>
			</tr>
		</t:Form>
	</table>
</div>

</html>